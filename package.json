{
  "name": "phovea_vis",
  "description": "Utilitiy reposititory containing common visualizations (table, heatmap, scatterplot, axis, pie, distribution, ..)",
  "version": "2.1.1-SNAPSHOT",
  "author": {
    "name": "The Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://phovea.caleydo.org",
  "bugs": {
    "url": "https://github.com/phovea/phovea_vis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phovea/phovea_vis.git"
  },
  "main": "build/phovea_vis.js",
  "files": [
    "src",
    "index.js",
    "phovea.js",
    "phovea_registry.js",
    "build"
  ],
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p . 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "docs": "typedoc --options typedoc.json src/",
    "prebuild": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test",
    "pretest": "npm run compile",
    "test": "test ! -d tests || karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack --mode=development",
    "build": "webpack --mode=production",
    "start": "webpack-dev-server --mode=development --inline",
    "start:hot": "webpack-dev-server --mode=development --inline --hot",
    "watch": "webpack --mode=development --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/phovea_vis.tar.gz *"
  },
  "dependencies": {
    "@types/d3": "~3.5.36",
    "d3": "~3.5.17",
    "phovea_core": "github:phovea/phovea_core#stoiber/webpack4",
    "phovea_d3": "github:phovea/phovea_d3#stoiber/webpack4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-loader": "^0.5.5",
    "imports-loader": "^0.8.0",
    "jasmine": "^3.1.0",
    "jasmine-promises": "^0.4.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^1.0.0-beta.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "thread-loader": "^1.1.5",
    "ts-loader": "^4.2.0",
    "tslib": "1.9.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "ifdef-loader": "^2.0.0"
  }
}

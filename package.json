{
  "name": "caleydo_vis",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_vis",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_core": "*",
    "caleydo_d3": "*",
    "caleydo_tooltip": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "vis",
          "id": "axis",
          "name": "Axis",
          "file": "axis",
          "icon": "axis_icon.svg",
          "scaling": "height-only",
          "filter": [
            "vector",
            "(real|int)"
          ],
          "options": {
            "tickSize": {
              "type": "int",
              "range": [
                1,
                null
              ],
              "default": 2
            },
            "orient": {
              "type": "categorical",
              "categories": [
                "left",
                "right",
                "top",
                "bottom"
              ],
              "default": "left"
            }
          }
        },
        {
          "type": "vis",
          "id": "barplot",
          "name": "Bar Plot",
          "file": "barplot",
          "icon": "barplot_icon.png",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "filter": [
            "vector",
            "(real|int)"
          ]
        },
        {
          "type": "vis",
          "id": "table",
          "name": "Table",
          "file": "table",
          "filter": "(matrix|table|vector)",
          "sizeDependsOnDataDimension": true
        },
        {
          "type": "vis",
          "id": "scatterplot",
          "name": "ScatterPlot",
          "file": "scatterplot",
          "filter": "matrix"
        },
        {
          "type": "vis",
          "id": "caleydo-vis-heatmap",
          "name": "HeatMap",
          "file": "heatmap",
          "icon": "heatmap_icon.svg",
          "sizeDependsOnDataDimension": true,
          "filter": "matrix"
        },
        {
          "type": "vis",
          "id": "caleydo-vis-heatmap1d",
          "name": "HeatMap 1D",
          "file": "heatmap",
          "icon": "heatmap_icon.svg",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "scaling": "height-only",
          "filter": "vector"
        },
        {
          "type": "vis",
          "id": "caleydo-vis-histogram",
          "file": "distribution",
          "name": "Histogram",
          "icon": "distribution_histogram_icon.png",
          "filter": [
            "(vector|matrix|stratification)",
            "(categorical|real|int)"
          ]
        },
        {
          "type": "vis",
          "id": "caleydo-vis-mosaic",
          "file": "distribution",
          "name": "Mosaic",
          "factory": "createMosaic",
          "icon": "distribution_mosaic_icon.png",
          "sizeDependsOnDataDimension": [
            false,
            true
          ],
          "scaling": "height-only",
          "filter": [
            "(vector|stratification)",
            "categorical"
          ]
        },
        {
          "type": "vis",
          "id": "caleydo-vis-pie",
          "file": "distribution",
          "name": "Pie",
          "factory": "createPie",
          "icon": "distribution_pie_icon.png",
          "scaling": "aspect",
          "filter": [
            "(vector|stratification)",
            "categorical"
          ]
        },
        {
          "type": "vis",
          "id": "caleydo-vis-box",
          "name": "BoxPlot",
          "file": "box",
          "icon": "box_icon.png",
          "scaling": "aspect",
          "filter": [
            "vector",
            "(real|int)"
          ]
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5"
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
